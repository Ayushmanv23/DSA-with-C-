#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s = "jhon12Doel14@gmail.com";
    unordered_map<char, int> countMap;

    for(char c:s)
    {
        if(isdigit(c))
        {
            countMap[c]++;
        }
    }
    for(const auto& entry:countMap)
    {
        cout<<entry.first<<" : "<<entry.second<<endl;
    }
    return 0;
}
