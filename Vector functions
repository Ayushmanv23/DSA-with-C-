#include<bits/stdc++.h>
#include<vector>
using namespace std;
int main()
{
    vector<int> v = {2, 5, 7, 1, 9};
    int size = v.size();
    cout<<"The size of vector is "<<size<<endl;
    v.push_back(10);
    cout<<"Capacity: "<<v.size()<<endl;
    v.push_back(12);
    cout<<"Capacity: "<<v.size()<<endl;
    v.push_back(14);
    cout<<"Capacity: "<<v.size()<<endl;
    cout<<"The first element is "<<v.front()<<endl;
    cout<<"The last element is "<<v.back()<<endl;

    cout<<"Before POP: ";
    for(int i:v)
    {
        cout<<i<<" ";
    }
    cout<<endl;

    v.pop_back();
    cout<<"After POP : ";
    for(int i:v)
    {
        cout<<i<<" ";
    }

}
